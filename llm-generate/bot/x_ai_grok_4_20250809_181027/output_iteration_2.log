                    return PokerAction.CALL, 0
            elif best_type >= 1:
                if to_call == 0:
                    amount = int(pot / 2)
                    amount = max(min_r, amount)
                    amount = min(amount, max_r)
                    if amount >= min_r and amount <= max_r:
                        return PokerAction.RAISE, amount
                    return PokerAction.CHECK, 0
                else:
                    if to_call < pot / 2:
                        return PokerAction.CALL, 0
                    return PokerAction.FOLD, 0
            else:
                if to_call == 0:
                    return PokerAction.CHECK, 0
                return PokerAction.FOLD, 0

